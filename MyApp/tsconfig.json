{
  "compilerOptions": {
    "noImplicitAny": false,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "lib": [ "es2015", "dom" ]
  },
  "exclude": [
    "bin",
    "node_modules",
    "wwwroot",
    "**/*.spec.ts"
  ]
}
