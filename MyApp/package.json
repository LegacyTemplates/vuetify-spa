{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "npm run build",
    "dev": "vue-cli-service build --watch",
    "serve": "vue-cli-service serve",
    "dtos": "cd src && x typescript",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "publish": "vue-cli-service build && dotnet publish -c Release",
    "test": "vue-cli-service test:unit",
    "test-watch": "vue-cli-service test:unit --watchAll",
    "e2e": "vue-cli-service test:e2e"
  },
  "dependencies": {
    "@servicestack/client": "^1.0.51",
    "es6-shim": "^0.35.6",
    "font-awesome": "^4.7.0",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.4.9",
    "vuetify": "^2.3.19"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.12.1",
    "@mdi/font": "^5.8.55",
    "@types/jest": "^26.0.16",
    "@vue/cli-plugin-babel": "^4.5.9",
    "@vue/cli-plugin-e2e-cypress": "^4.5.9",
    "@vue/cli-plugin-typescript": "^4.5.9",
    "@vue/cli-plugin-unit-jest": "^4.5.9",
    "@vue/cli-service": "^4.5.9",
    "@vue/test-utils": "^1.1.1",
    "babel-core": "^6.26.3",
    "gulp": "^4.0.2",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.1.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2",
    "vue-cli-plugin-vuetify": "^2.0.8",
    "vue-template-compiler": "^2.6.12"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>[/\\\\](node_modules|wwwroot|bin)[/\\\\]"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!vue-router)"
    ],
    "testURL": "http://localhost:5000/",
    "setupTestFrameworkScriptFile": "<rootDir>/tests/jest-setup.js"
  }
}
